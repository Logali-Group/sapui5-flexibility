<core:FragmentDefinition 
                        xmlns="sap.m" 
                        xmlns:core="sap.ui.core">

	<SelectDialog
		id="selectDialog"
		title="{i18n>countries}"
		items="{/VH_Countries}"
		search=".onValueHelpSearch"
		confirm=".onCountryValueHelpConfirm"
		cancel=".onCountryValueHelpCancel"
		multiSelect="true">

		<StandardListItem id="standardListItem"
			icon="{Flag}"
			iconDensityAware="false"
			iconInset="false"
			title="{Country}"
			description="{Code}">
	        <customData>
                <core:CustomData key="key" value="{ID}"/>
            </customData>
		</StandardListItem>

	</SelectDialog>

</core:FragmentDefinition>